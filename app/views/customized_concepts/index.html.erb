<p id="notice"><%= notice %></p>

<h1>Listing Customized Concepts</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Title</th>
      <th>Description</th>
      <th>Video</th>
      <th>Lecture</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @customized_concepts.each do |customized_concept| %>
      <tr>
        <td><%= customized_concept.course_id %></td>
        <td><%= customized_concept.title %></td>
        <td><%= customized_concept.description %></td>
        <td><%= customized_concept.video %></td>
        <td><%= customized_concept.lecture %></td>
        <td><%= link_to 'Show', customized_concept %></td>
        <td><%= link_to 'Edit', edit_customized_concept_path(customized_concept) %></td>
        <td><%= link_to 'Destroy', customized_concept, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Customized concept' %>
